PUSH poll_connected
{
	integer loop;
	for (loop = 1 to GetNumArrayCols(host_connected_fb))
		{
		makestring (TX$,"usb p0%d\x0D",loop);
		delay(10);
		}
}

PUSH poll_status
{
	makestring (TX$,"read\x0D");
}

PUSH host_take
{
    take_host(GetLastModifiedArrayIndex ());
}

PUSH priority_none
{
	makestring (TX$,"swmode priority off\x0D");
}

PUSH auto_switch_enable
{
	makestring (TX$,"pod on\x0D");
}

PUSH auto_switch_enable
{
	makestring (TX$,"pod on\x0D");
}


PUSH auto_switch_disable
{
	makestring (TX$,"pod off\x0D");
}

CHANGE host
{
    take_host(host);
}

CHANGE priority
{
   	makestring (TX$,"swmode priority i0%d\x0D",priority);

}

