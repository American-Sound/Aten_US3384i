Function take_host(integer newhost)
{
   	makestring (TX$,"sw i0%d\x0D",newhost);
}

Function update_host_fb (integer curhost)
{
	integer loop;	
	for (loop = 1 to GetNumArrayCols(host_taken_fb))
		{
		if (loop = curhost)
			{
			host_taken_fb[loop] = 1;
			host_fb = loop;
			}
			else
				{
				host_taken_fb[loop] = 0;				
				}
		}
}

Function update_priority_fb (integer priority_host)
{
	priority_fb = priority_host;
	priority_none_fb = !priority_host;
}

Function update_auto_switch_enable_fb (integer auto_switch_enable_state)
{
	auto_switch_enable_fb = auto_switch_enable_state;
	auto_switch_disable_fb = !auto_switch_enable_state;

}
