*/
THREADSAFE CHANGE RX$
{
	String tmpString[65534];
	String Chomp$[32];
	integer which_port;
 
	While(1) // This While(1) is OK 
	{ 
		Try // Prevents an exception from exiting the event 
		{ 
		tmpString = Gather("\r", RX$); //Gather keeps the priority high until delimiter is found 
		// processing of tmpString
		trace ("parsing tmpString %s\n",tmpString);
		if (FIND("Command OK", tmpString))
			{
			trace ("Found Command OK in tmpString\n");
			if (FIND("sw i", tmpString))
				{
				Chomp$ = REMOVE ("C",tmpString);
            	update_host_fb (ATOI(Chomp$));
            	trace ("Updating feedback with host = %d",ATOI(Chomp$));
				}
				else if (FIND("pod on", tmpString))
					{	
					update_auto_switch_enable_fb(1);
					}
					else if (FIND("pod off", tmpString))
						{	
						update_auto_switch_enable_fb(0);
						}
						else if (FIND("swmode priority", tmpString))
							{
							if (FIND("off", tmpString))
								{
								update_priority_fb (0);	
								}
								else if (FIND("i0", tmpString))
									{
									update_priority_fb (ATOI(tmpString));	
									}
							}

			}

		if (FIND("Port:", tmpString) && FIND("connected)", tmpString))
			{
			trace ("Found connection info in tmpString\n");
			Chomp$ = REMOVE (")",tmpString);
			which_port=ATOI(Chomp$);
			trace ("Working with message %s, port = %d\n",Chomp$,which_port);
			if (FIND("Current", Chomp$)) // a reply to the "read" command, both current host and connection status
			  	{
            	update_host_fb (which_port);		  	
			  	}
			if (FIND("(not", Chomp$))
				{
				host_connected_fb[which_port] = 0;
				}
				else if (FIND("(connected", Chomp$))
					{
					host_connected_fb[which_port] = 1;							
					}
            }
			else if (FIND("POD: OFF", tmpString))
				{
				update_auto_switch_enable_fb(0);
				}
				else if (FIND("POD: ON", tmpString))
					{
					update_auto_switch_enable_fb(1);
					}
					else if (FIND("Priority Port:", tmpString))
						{
						update_priority_fb (ATOI(tmpString));	
						}

					            
		tmpString = "";
		} 
 		Catch
		{
		} 
	}      
}

